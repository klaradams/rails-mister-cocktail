<div class="form_container">
  <h1> New Cocktail: </h1>

  <%= simple_form_for(@cocktail) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
  </div>
    <%= f.submit %>
  <% end %>
  <p><%= link_to "Back", cocktails_path, class: 'btn'%></p>
</div>
